{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/ws",
      "dest": "server.js",
      "headers": {
        "Upgrade": "websocket",
        "Connection": "upgrade"
      }
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}